{
    "name": "sulu/activity-log-bundle",
    "description": "Sulu bundle for logging activities",
    "type": "sulu-bundle",
    "license": "MIT",
    "require": {
        "php": "^5.5 || ^7.0",
        "sulu/sulu": "^1.1 || dev-develop"
    },
    "require-dev": {
        "zendframework/zend-stdlib": "2.3.1 as 2.0.0rc5",
        "zendframework/zendsearch": "2.*@dev",
        "symfony/monolog-bundle": "2.4.*",
        "doctrine/doctrine-bundle": "1.4.*",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "phpunit/phpunit": "^4.8 || ^5.0",

        "sulu/document-manager": "@dev",
        "massive/search-bundle": "@dev"
    },
    "keywords": [
        "activities",
        "activity-log",
        "logging"
    ],
    "authors": [
        {
            "name": "Sulu Activity Log",
            "homepage": "https://github.com/sulu/SuluActivityLogBundle/contributors"
        }
    ],
    "homepage": "https://github.com/sulu/SuluActivityLogBundle",
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ActivityLogBundle\\": ""
        }
    }
